{
    "builds" : [{
        "src" : "registration/wsgi.py",
        "use" : "@versel/python",
        "config" : {"maxLambdaSize" : "15mb", "runtime" : "python3.12"}
    }],
    "routes" : [
        {
            "src" : "/(.*)", 
            "dest" : "registration/wsgi.py"
        }
    ]
}